---
import frontEndImage from "@assets/section1-frontend.svg";
---

<img
      class="image-frontend"
      src={frontEndImage.src}
      alt="Frontend"
      fetchpriority="high"
      loading="eager"
      height="275"
    />

<style>
    .image-frontend {
      position: absolute;
      z-index: 2;
      bottom: -5px;
      max-width: 1320px;
      transform: translateY(100%);
      animation: frontend-slide-up 1s 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275) forwards;
      height: auto;
      aspect-ratio: 1920/275;
    }
    @keyframes frontend-slide-up {
    0% {
      transform: translateY(100%);
    }
    100% {
      transform: translateY(0%);
    }
  }
</style>
